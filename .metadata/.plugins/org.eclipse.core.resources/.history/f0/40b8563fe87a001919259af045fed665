
public class FieldInitValue {

		// 필드
		byte byteField;
		short shortField;
		int intfield;
		long longField;
		
		boolean booleanField;
		char charField;
		
		float floatField;
		double doubleField;
		
		int[] arrField;
		String referenceField;


		public static void main(String[] args) {
			FieldInitValue f = new FieldInitValue();
			System.out.println(byte.byteField);
			
	}

}
