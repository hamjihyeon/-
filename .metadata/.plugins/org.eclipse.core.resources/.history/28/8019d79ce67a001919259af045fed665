
public class FieldInitValue {

		// 필드
		byte byteField;
		short shortField;
		int intfield;
		long longField;
		
		boolean booleanField;
		char charField;
		
		float floatField;
		double doubleField;
		
		int[] arrField;
		String referenceField;
		
		public FieldInitValue(byte b, short s, int i, long l, boolean bo, char c, float f, double d, int[] a, String r) {
			byteField = b;
			shortField = s;
			intfield = i;
			longField = l;
			booleanField = bo;
			charField = c;
			floatField = f;
			doubleField = d;
			arrField = a;
			referenceField = r;
		}

		public static void main(String[] args) {
	}

}
